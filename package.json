{
  "name": "@conorw/conorw-contracts",
  "version": "1.0.0",
  "description": "NFT Collection Factory",
  "license": "UNLICENSED",
  "files": [
    "contracts"
  ],
  "scripts": {
    "build": "yarn compile && npx tsc",
    "compile": "npx hardhat compile --force",
    "clean": "rm -rf dist && npx hardhat clean",
    "coverage": "npx hardhat coverage",
    "deploy": "npx hardhat deploy ",
    "format": "prettier --write .",
    "format:check": "prettier --check '*/**/*.{js,sol,json,md,ts}'",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "test": "npx hardhat test",
    "verify": "npx hardhat etherscan-verify"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "3.1.0",
    "@nomiclabs/hardhat-waffle": "2.0.3",
    "@openzeppelin/contracts": "4.8.2",
    "@openzeppelin/contracts-upgradeable": "4.8.2",
    "@tsconfig/recommended": "1.0.1",
    "@typechain/ethers-v5": "10.1.0",
    "@typechain/hardhat": "6.1.2",
    "@types/chai": "4.3.1",
    "@types/mocha": "9.1.1",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "chai": "4.3.6",
    "dotenv": "16.0.1",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "ethereum-waffle": "3.4.4",
    "ethers": "5.6.9",
    "hardhat": "^2.14.0",
    "hardhat-contract-sizer": "^2.7.0",
    "hardhat-deploy": "^0.11.28",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "8.0.1",
    "jsbi": "^4.3.0",
    "lint-staged": "13.0.2",
    "node-fetch": "3.2.6",
    "pinst": "^3.0.0",
    "prettier": "2.5.1",
    "prettier-plugin-solidity": "1.0.0-beta.24",
    "solhint": "3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.8.2",
    "ts-generator": "0.1.1",
    "ts-node": "10.8.1",
    "typechain": "8.1.0",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint -c .eslintrc.json"
  },
  "keywords": [
    "conorw",
    "nft",
    "erc721"
  ],
  "author": "ConorW"
}
